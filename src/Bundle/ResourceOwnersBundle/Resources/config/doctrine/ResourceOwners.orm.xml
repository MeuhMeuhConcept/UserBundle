<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <mapped-superclass name="MMC\User\Bundle\ResourceOwnersBundle\Entity\ResourceOwners">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
        </id>

        <field name="resourceOwnerName" column="resource_owner_name" type="string" nullable="true" />

        <field name="resourceOwnerId" column="resource_owner_id" type="string" nullable="true" />

        <field name="resourceOwnerAccessToken" column="resource_owner_access_token" type="string" nullable="true" />

    </mapped-superclass> 
                                                                                                   
</doctrine-mapping>
